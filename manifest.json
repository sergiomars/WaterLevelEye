{
  "name": "Water Level Eye",
  "description": "A simple water level app.",
  "manifest_version": 2,
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.hydrodaten.admin.ch/"
  ]
}