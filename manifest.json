{
  "name": "Water Level Eye",
  "description": "A simple water level app.",
  "manifest_version": 2,
  "version": "1.2",
  "browser_action": {
    "default_icon": "resources/images/icon.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background":
  {
    "scripts":["js/background.js"]
  },
  "permissions": [
    "https://www.hydrodaten.admin.ch/",
    "storage",
    "notifications"
  ]
}